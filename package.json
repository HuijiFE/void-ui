    "auto:types": "node auto/types.js",
    "auto:docs": "node auto/docs.js",
    "auto": "npm run auto:lib && npm run auto:types && npm run auto:docs",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.config.development.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "jest --config test/unit/jest.conf.js --coverage",
    "eslint": "eslint --ext .js src test/unit/specs test/e2e/specs",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint": "npm run eslint && npm run tslint",
    "build:lib": "node build/build-lib.js",
    "build:docs": "node build/build-docs.js",
    "build": "npm run build:lib && npm run build:docs"
