{
  "name": "void-ui",
  "description": "A UI toolkit for Vue.js.",
  "version": "0.1.5",
  "keywords": [
    "vue",
    "typescript",
    "components",
    "ui",
    "huiji"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "homepage": "http://void.huijiwiki.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/HuijiFE/void-ui"
  },
  "bugs": {
    "url": "https://github.com/HuijiFE/void-ui/issues"
  },
  "files": [
    "dist",
    "public",
    "src",
    "types"
  ],
  "main": "dist/void-ui.common.js",
  "module": "dist/void-ui.common.js",
  "types": "types/index.d.ts",
  "scripts": {
    "ts": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json",
    "gen-files": "yarn ts scripts/gen-files.ts",
    "gen-fields": "yarn ts scripts/gen-fields.ts",
    "serve": "cross-env BUILD_SOLUTION=docs vue-cli-service serve docs/main.ts --open --port 8102",
    "build:lib": "cross-env BUILD_SOLUTION=lib vue-cli-service build lib/void-ui.ts --target lib",
    "build:docs": "cross-env BUILD_SOLUTION=docs vue-cli-service build docs/main.ts --modern --report",
    "build": "yarn build:lib && yarn build:docs",
    "test": "vue-cli-service test",
    "lint": "stylelint **/*.scss && vue-cli-service lint"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "popmotion": "^8.3.3",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.0.0"
  },
  "peerDependencies": {
    "@vue/cli-plugin-typescript": "^3.0.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/globby": "^8.0.0",
    "@types/jest": "^23.3.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.5.8",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.9",
    "@types/webpack-chain": "^4.8.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-typescript": "^3.0.0",
    "@vue/cli-plugin-unit-jest": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/test-utils": "^1.0.0-beta.24",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "cross-env": "^5.2.0",
    "ejs": "^2.6.1",
    "globby": "^8.0.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.0",
    "typescript": "^3.0.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0",
    "yarn": ">= 1.9.0"
  }
}
