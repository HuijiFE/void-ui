{
  "name": "void-ui",
  "description": "A UI toolkit for Vue.js.",
  "version": "0.1.2-alpha.1",
  "keywords": [
    "vue",
    "typescript",
    "components",
    "ui",
    "huiji"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "homepage": "http://void.huijiwiki.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/HuijiFE/void-ui"
  },
  "bugs": {
    "url": "https://github.com/HuijiFE/void-ui/issues"
  },
  "files": [
    "dist",
    "public",
    "src",
    "types"
  ],
  "main": "dist/void-ui.common.js",
  "module": "dist/void-ui.common.js",
  "types": "types/index.d.ts",
  "scripts": {
    "ts": "ts-node -P scripts/tsconfig.json",
    "gen-files": "yarn ts scripts/gen-files.ts",
    "gen-variables": "yarn ts scripts/gen-variables.ts",
    "gen-types": "yarn ts scripts/gen-types.ts --pre && tsc -p types/tsconfig.json --emitDeclarationOnly",
    "serve": "VUE_SOLUTION=docs vue-cli-service serve docs/main.ts",
    "build:void": "VUE_SOLUTION=void vue-cli-service build src/VoidUI.ts --target lib",
    "build:docs": "VUE_SOLUTION=docs vue-cli-service build docs/main.ts",
    "build": "yarn build:void && yarn build:docs",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "vue:help": "vue-cli-service -h"
  },
  "dependencies": {
    "normalize.css": "^8.0.0",
    "popmotion": "^8.1.22",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-brands": "^5.0.12",
    "@fortawesome/fontawesome-free-regular": "^5.0.12",
    "@fortawesome/fontawesome-free-solid": "^5.0.12",
    "@fortawesome/vue-fontawesome": "^0.0.22",
    "@types/ejs": "^2.5.1",
    "@types/globby": "^6.1.0",
    "@types/jest": "^22.2.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.0.2",
    "@types/rimraf": "^2.0.2",
    "@types/webpack-chain": "^4.0.2",
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.9",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "@vue/test-utils": "^1.0.0-beta.10",
    "babel-core": "^7.0.0-0",
    "ejs": "^2.5.9",
    "globby": "^8.0.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuex": "3.0.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
